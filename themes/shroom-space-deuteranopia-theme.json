  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#726D89",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#E8C990"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#BE9AF7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#E8C990",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#E8C990"
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#E68484",
        "fontStyle": "bold"
      }
    }
  ],
  "semanticTokenColors": {
    "enabled": true,
    "rules": {
      "parameter": {
        "foreground": "#82AAFF"
      },
      "method": {
        "foreground": "#82AAFF"
      },
      "type": {
        "foreground": "#E8C990",
        "fontStyle": "italic"
      },
      "class": {
        "foreground": "#E8C990",
        "fontStyle": "italic"
      },
      "variable": {
        "foreground": "#82AAFF"
      }
    }
  }
