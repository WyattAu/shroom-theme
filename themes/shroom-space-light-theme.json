  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#948E9E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#5A785A"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#8B5F9E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#8B5F9E",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#007A7A"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#8C6D34",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#8C6D34"
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#585FB5"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#4A4555"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#B45353",
        "fontStyle": "bold"
      }
    }
  ],
  "semanticTokenColors": {
    "enabled": true,
    "rules": {
      "parameter": {
        "foreground": "#585FB5"
      },
      "property": {
        "foreground": "#4A4555"
      },
      "method": {
        "foreground": "#007A7A"
      },
      "type": {
        "foreground": "#8C6D34",
        "fontStyle": "italic"
      },
      "class": {
        "foreground": "#8C6D34",
        "fontStyle": "italic"
      },
      "variable": {
        "foreground": "#585FB5"
      }
    }
  }
}