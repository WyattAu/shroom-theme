  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#777777",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#EEEEEE",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#550000"
      }
    }
  ],
  "semanticTokenColors": {
    "enabled": true,
    "rules": {
      "parameter": {
        "foreground": "#DDDDDD"
      },
      "method": {
        "foreground": "#DDDDDD"
      },
      "type": {
        "foreground": "#EEEEEE",
        "fontStyle": "italic"
      },
      "class": {
        "foreground": "#EEEEEE",
        "fontStyle": "italic"
      },
      "variable": {
        "foreground": "#DDDDDD"
      }
    }
  }
}