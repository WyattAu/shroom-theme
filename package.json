{
  "name": "shroom-space-theme",
  "displayName": "Shroom Space Theme",
  "description": "VS Code theme extension",
  "version": "0.0.1",
  "publisher": "wyattau",
  "author": "Wyatt Au <wyatt_au@protonmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WyattAu/shroom-theme.git"
  },
  "keywords": [
    "vscode",
    "theme",
    "shroom",
    "space"
  ],
  "homepage": "https://github.com/WyattAu/shroom-theme#readme",
  "bugs": {
    "url": "https://github.com/WyattAu/shroom-theme/issues"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Shroom Space",
        "uiTheme": "vs-dark",
        "path": "./themes/shroom-space-theme.json"
      },
      {
        "label": "Shroom Space (Deuteranopia)",
        "uiTheme": "vs-dark",
        "path": "./themes/shroom-space-deuteranopia-theme.json"
      },
      {
        "label": "Shroom Space (Protanopia)",
        "uiTheme": "vs-dark",
        "path": "./themes/shroom-space-protanopia-theme.json"
      },
      {
        "label": "Shroom Space (Tritanopia)",
        "uiTheme": "vs-dark",
        "path": "./themes/shroom-space-tritanopia-theme.json"
      },
      {
        "label": "Shroom Space (Monochrome)",
        "uiTheme": "vs-dark",
        "path": "./themes/shroom-space-monochrome-theme.json"
      },
      {
        "label": "Shroom Space (High Contrast)",
        "uiTheme": "vs-dark",
        "path": "./themes/shroom-space-high-contrast-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.2",
    "chai": "^6.2.0",
    "eslint": "^9.36.0",
    "sinon": "^21.0.0",
    "typescript": "^5.9.3"
  }
}
